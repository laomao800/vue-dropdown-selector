{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "env": {
    "development": {
      "plugins": ["transform-decorators-legacy", "transform-class-properties", "transform-vue-jsx", "transform-runtime"]
    },
    "production": {
      "presets": ["env"],
      "plugins": ["transform-decorators-legacy", "transform-class-properties", "transform-vue-jsx", "babel-plugin-add-module-exports"]
    },
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["transform-decorators-legacy", "transform-class-properties", "transform-vue-jsx", "transform-es2015-modules-commonjs", "dynamic-import-node"]
    }
  }
}
